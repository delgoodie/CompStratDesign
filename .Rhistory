generate_gb_covariance <- function(g, b, sigma = 0.01, cor = TRUE) {
p <- length(b)
stopifnot(igraph::vcount(g) == p)
stopifnot(b >= 0, b == round(b))
D <- igraph::shortest.paths(g)
b <- pmin(b, apply(D, 1, max))
A <- D <= b # A_jk indicates whether k is in N_j
B <- A | t(A)
Sig <- B / D
diag(Sig) <- 0
# add to diag so min eval=sig^2
diag(Sig) <- max(-min(eigen(Sig)$val), 0) + sigma^2
if (cor) {
sig <- sqrt(diag(Sig))
D <- diag(1 / sig)
Sig <- D %*% Sig %*% D
}
Sig
}
ggb_tester <- function(S, g, lambda = NULL, flmin = 0.01,  max_depths = NULL, maxiter = 500, tol = 1e-4, verbose = 1) {
D <- igraph::shortest.paths(g)
D[D == Inf] <- -1
M <- apply(D, 1, max)
if (!is.null(max_depths)) {
stopifnot(length(max_depths) == igraph::vcount(g),
max_depths >= 0,
max_depths == round(max_depths))
M <- pmin(M, max_depths)
}
D[D == -1] <- max(M) + 1 # instead of Inf, we use 1 larger than all the M's.
out <- prox_bcd_test(R = S, D = D, M = M, lambda = lambda, flmin = flmin, maxiter = maxiter, tol = tol, verbose = verbose)
return(list(Sig = out$sumV, lambda = out$lambda, obj = out$obj, type='local'))
}
